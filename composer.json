{
	"name": "alfredoramos/hide",
	"type": "phpbb-extension",
	"description": "Hide BBCode extension for phpBB",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-hide",
	"version": "2.1.0",
	"time": "2020-05-05",
	"keywords": [
		"phpbb",
		"extension",
		"hide",
		"bbcode"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@proton.me",
			"homepage": "https://alfredoramos.mx",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": "^8.1.0",
		"composer/installers": "^2.2.0"
	},
	"require-dev": {
		"phing/phing": "^3.1.1",
		"pear/archive_tar": "^1.4.0"
	},
	"extra": {
		"display-name": "Hide",
		"soft-require": {
			"phpbb/phpbb": "~3.3.0"
		},
		"version-check": {
			"host": "alfredoramos.mx",
			"directory": "/phpbb-extensions/versions",
			"filename": "hide.json",
			"ssl": true
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}